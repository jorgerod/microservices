spring:
  application:
    name: eureka-server
server:
  port: 8761
info:
  version: 0.0.1-SNAPSHOT
  Description: Eureka Server
eureka:
  client:
    registerWithEureka: true
    fetchRegistry: true
    serviceUrl:
      defaultZone: http://localhost:8761/eureka/
  server:
    waitTimeInMsWhenSyncEmpty: 0
    enableSelfPreservation: true
    